function pwBreak(pw, cmp){
  var sum = sumitup(cmp);
  var pwEn = pw;
  var pwEnArr = pwEn.split('');
  
  var pwBuild = [];
  for(i=0;i<pwEn.length;i++){
    pwBuild.push(pwEnArr.shift());
    pwEnArr.splice(0,sum);
    
  }
  return pwBuild;
}
