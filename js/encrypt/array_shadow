function encrypt(pw){
  var tmp = pw.split('');
  var clientArr = [];
  tmp.map(function(val){return clientArr.push(' ');});
  
  var encrypted = tmp.map(function(val){
    return val;
  });
  
  console.log(pw);
  console.log(tmp);
  console.log(clientArr);
  console.log(encrypted.join(''));
}

encrypt('sooooop');
